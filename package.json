  },
  "mocha": {
    "require": [
      "@babel/register",
      "chai/register-expect",
      "jsdom-global/register",
      "ts-node/register"
    ],
    "file": [
      "test/setup.js"
    ],
    "spec": "./test/**/*.test.[jt]s?(x)"
  },
  "browserslist": [
    "last 2 chrome versions",
    "last 2 firefox versions",
    "> 2%",
    "Firefox ESR",
    "not dead"
  ]
