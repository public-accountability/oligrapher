{
  "name": "@public-accountability/oligrapher",
  "version": "3.0.0-beta",
  "description": "Network graph visualizer built for LittleSis",
  "main": "app/Oligrapher.jsx",
  "scripts": {
    "build-dev": "webpack --color --progress --env.development",
    "build-prod": "webpack --color --progress --env.production",
    "dev-server": "webpack-dev-server --color --progress --env.dev_server --env.api_url=http://localhost:8081",
    "build": "yarn run build-dev && yarn run build-prod",
    "test": "TS_NODE_COMPILER_OPTIONS='{\"module\":\"commonjs\"}' NODE_ENV=test mocha",
    "profile": "webpack --profile --display-modules --env.production",
    "lint": "yarn run lint:js && yarn run lint:scss",
    "lint:js-fix": "eslint app --ext .js --ext .jsx --fix",
    "lint:js": "eslint app --ext .js --ext .jsx",
    "lint:scss": "stylelint app",
    "upgrade": "yarn upgrade-interactive --latest && yarn upgrade",
    "explore-source-map": "webpack --color --progress --env.production --env.sourcemap && source-map-explorer dist/oligrapher.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/public-accountability/oligrapher"
  },
  "author": "Public Accountability Initiative",
  "license": "GPL-3.0",
  "bugs": {
    "url": "https://github.com/public-accountability/oligrapher/issues"
  },
  "homepage": "https://littlesis.org/oligrapher",
  "dependencies": {
    "@ant-design/icons": "^4.3.0",
    "@ckeditor/ckeditor5-build-classic": "^19.0.2",
    "@ckeditor/ckeditor5-react": "^2.1.0",
    "@hot-loader/react-dom": "^16.13.0",
    "@loadable/component": "^5.13.1",
    "@material-ui/core": "^4.9.14",
    "@public-accountability/littlesis-api": "^0.0.7",
    "immer": "^8.0.0",
    "lodash": "^4.17.15",
    "prop-types": "^15.7.2",
    "react": "^16.13.0",
    "react-beautiful-dnd": "^13.0.0",
    "react-color": "^2.19.3",
    "react-dom": "^16.13.0",
    "react-draggable": "^4.2.0",
    "react-hotkeys-hook": "^2.4.0",
    "react-icons": "^3.10.0",
    "react-redux": "^7.2.2",
    "react-resize-detector": "^4.2.3",
    "redux": "^4.0.5",
    "redux-logger": "^3.0.6",
    "redux-saga": "^1.1.3",
    "redux-undo": "^1.0.1",
    "shortid": "^2.2.16",
